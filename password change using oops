class Account:
    def __init__(self, name, acc_no, password):  
        self.acc_no = acc_no
        self.password = password
        self.name = name

    def reset_password(self, new_password):
        self.password = new_password
        print("Password successfully changed!")

acc1 = Account("Shubham", 123456789, 1234)

print("The account is registered in the name of : " ,acc1.name)      
print("Customrt account number : " ,acc1.acc_no)


acc1.reset_password(5678)


print("The new password for your account is " ,acc1.password)  
